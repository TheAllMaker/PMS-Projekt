<Window x:Class="MediTrack.View.RemoteView.AddPatientWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MediTrack.View.RemoteView"
        mc:Ignorable="d"
        Background="Black"
        Name="Test"
        Title="AddPatientWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Vorname:" />
        <TextBox x:Name="txtFirstName" />
        <Label Content="Nachname:" />
        <TextBox x:Name="txtLastName" />
        <Label Content="txtPatientNumber:" />
        <TextBox x:Name="txtPatientNumber" />
        <Button Content="Speichern" Click="SaveButton_Click" />
        <Button Content="Abbrechen" Click="CancelButton_Click" />
        <Button Content="Abbrechen" Click="CancelButton_Click" />
    </Grid>
</Window>
